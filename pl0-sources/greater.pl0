(* -*- mode:pascal; -*-
 * Exemplo de programa em PL/0.
 * Realiza comparação se valor a é maior que b.
 *)

var x, y;

procedure greater(a, b);
(* var a, b, r; *)
var r;
begin
  (*
  a := x;
  b := y;
  *)
  if a > b then r := a;
  if b > a then r := b;
  !r
end;

begin
  ?x;
  ?y;
  call greater(x, y)
end.
