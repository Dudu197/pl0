(* -*- mode:pascal; -*-
 * Exemplo de programa em PL/0.
 * Realiza comparação se valor a é maior que b.
 *)

var x, y;

procedure greater(c, d);
var a, b, r;
begin
  a := x;
  b := y;
  if a > b then r := a;
  if b > a then r := b;
  !r
end;

begin
  (*
